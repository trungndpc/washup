{
  "name": "washupclient",
  "version": "1.0.0",
  "description": "A Javascript Framework for WebApp. React, Redux, Redux Saga, ES6, ...",
  "engines": {
    "npm": ">=6"
  },
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message",
    "start": "npm-run-all --parallel open:src",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "build:production": "npm run clean-dist && babel-node tools/build.js && npm run open:dist",
    "build:staging": "npm run clean-dist && babel-node tools/buildStaging.js && npm run open:dist",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "format": "prettier --write --single-quote --no-semi --print-width 160  'app/**/*.js'"
  },
  "author": "MinhNVC",
  "dependencies": {
    "@babel/preset-env": "^7.1.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-owl-carousel2": "^0.3.0",
    "react-redux": "5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "4.0.8",
    "react-s-alert": "^1.4.1",
    "react-select": "^3.1.0",
    "redux": "4.0.0",
    "redux-saga": "0.16.0",
    "request": "^2.79.0",
    "whatwg-fetch": "^3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "autoprefixer": "9.1.5",
    "babel-loader": "^8.0.2",
    "browser-sync": "^2.26.12",
    "chai": "4.1.2",
    "chalk": "^2.4.1",
    "connect-history-api-fallback": "1.5.0",
    "coveralls": "3.0.2",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "enzyme": "3.6.0",
    "eslint": "5.6.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-watch": "4.0.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "isparta": "4.1.0",
    "istanbul": "0.4.5",
    "jsdom": "12.0.0",
    "json-loader": "0.5.7",
    "lodash": "^4.17.20",
    "mocha": "5.2.0",
    "mockdate": "2.0.2",
    "npm-run-all": "4.1.3",
    "open": "0.0.5",
    "prettier": "^1.14.3",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.6.2",
    "redux-immutable-state-invariant": "2.1.0",
    "replace": "1.0.0",
    "rimraf": "2.6.2",
    "sinon": "6.3.4",
    "sinon-chai": "3.2.0",
    "style-loader": "0.23.0",
    "url-loader": "1.1.1",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-dev-middleware": "3.3.0",
    "webpack-hot-middleware": "2.24.0",
    "webpack-md5-hash": "0.0.6"
  }
}
